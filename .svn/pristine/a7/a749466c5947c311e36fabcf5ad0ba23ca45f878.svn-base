  .countdown {
    font-size: 34px;
    position: absolute;
    top: 2%;
    left: 3%;
    color: #fff;
  }
  .game_num {
    font-size: 26px;
    position: absolute;
    width: 140px;
    top: 11%;
    background: #e03d21;
    padding: 0 10px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    left: ~'calc(50% - 65px)';
    color: #fff;
    letter-spacing: 2px;
    text-align: center;
    font-weight: 500;
    span {
      font-size: 28px;
    }
  }
  .mask {
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    position: absolute;
    z-index: 3;
    background: rgba(10, 10, 10, .4);
    .btn-close {
      width: 7%;
      right: 7%;
      top: 18%;
    }
  }

  //未过关
  .mask-noWin {
    .panel-noWin {
      width: 80%;
      left: 10%;
      top: 26%;
    }
    .result_num {
      position: absolute;
      font-size: 26px;
      width: 100%;
      text-align: center;
      top: 36.5%;
    }
    .btn-yes {
      width: 29%;
      top: 66%;
      left: 12%;
    }
    .btn-no {
      width: 29%;
