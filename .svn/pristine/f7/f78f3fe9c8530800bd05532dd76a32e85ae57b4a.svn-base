<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <meta name="viewport"
          content="width=device-width,user-scalable=no, initial-scale=1, minimum-scale=1, maximum-scale=1">
    <!--强制竖屏-->
    <meta name="screen-orientation" content="portrait">
    <meta name="x5-orientation" content="portrait">
    <meta name="x5-page-mode" content="app">

    <link rel="stylesheet" href="assets/css/style.min.css">
</head>
<body>
<!--首页-->
<div class="page1 page">
    <img class="btn-start" src="assets/images/btn-start.png">
</div>
<!--游戏页-->
<div class="page2 page none">
    <img class="logo" src="assets/images/logo.png">
    <div class="regulation">
        <img class="panel-regulation" src="assets/images/p2-1.png">
        <img class="btn-close" src="assets/images/btn-close.png">
    </div>
    <img class="p22" src="assets/images/p2-2.jpg">

    <div class="dbw pab z1 lisCon" data-num='5'>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
        <em></em>
    </div>
    <img class="p23" src="assets/images/p2-3.jpg">
    <h1 class="game_num"><span>0</span>得分</h1>
    <h1 class="countdown">0s</h1>

    <!--游戏结束未过关-->
    <div class="mask-noWin mask none">
        <img class="panel-noWin" src="assets/images/panel-noWin.png">
        <p class="result_num"></p>
        <img class="btn-yes" src="assets/images/btn-yes.png">
        <img class="btn-no" src="assets/images/btn-no.png">
    </div>

    <!--游戏结过关-->
    <div class="mask-win mask none">
        <img class="panel-win" src="assets/images/panel-win.png">
        <img class="btn-toDraw" src="assets/images/btn-toDraw.png">
        <p class="result_num"></p>
    </div>

    <!--游戏结束机会用尽-->
    <div class="mask-noChance mask none">
        <img class="panel-noChance" src="assets/images/panel-noChance.png">
        <img class="btn-close" src="assets/images/btn-close.png">
        <img class="code" src="assets/images/code.png">
    </div>

    <!--抽奖-->
    <div class="mask-turntable mask none">
        <img class="turntable" src="assets/images/turntable.png">
        <div class="turntable-panel">
            <div class="top">
                <div class="pri1 pri active">
                    <img src="assets/images/p3-1.png">
                </div>
                <div class="pri2 pri">
                    <img src="assets/images/p3-2.png">
                </div>
                <div class="pri3 pri">
                    <img src="assets/images/p3-3.png">
                </div>
            </div>
            <div class="middle">
                <div class="pri8 pri">
                    <img src="assets/images/p3-8.png">
                </div>
                <div class="btn-draw">
                    <img src="assets/images/btn-draw.png">
                </div>
                <div class="pri4 pri">
                    <img src="assets/images/p3-4.png">
                </div>
            </div>
            <div class="bottom">
                <div class="pri7 pri">
                    <img src="assets/images/p3-7.png">
                </div>
                <div class="pri6 pri">
                    <img src="assets/images/p3-6.png">
                </div>
                <div class="pri5 pri">
                    <img src="assets/images/p3-5.png">
                </div>
            </div>
        </div>
    </div>
    
    <!--奖品-->
    <div class="mask-prize mask none">
        <img class="panel-prize" src="assets/images/panel-prize.png">
        <img class="btn-get" src="assets/images/btn-get.png">
        <p class="prize-name"></p>
        <img class="prize" src="assets/images/prize1.png">
    </div>

    <!--领奖信息-->
    <div class="mask-info mask">
        <img class="panel-info" src="assets/images/panel-info.png">
        <img class="btn-submit" src="assets/images/btn-submit.png">
        <form>
            <input type="text" name="user_name" placeholder="收件人">
            <input type="number" name="user_phone" placeholder="电话">
            <input type="text" name="user_address" placeholder="地址">
        </form>
    </div>

    <!--提交成功-->
    <div class="mask-submitok mask none">
        <img class="panel-submitok" src="assets/images/panel-submitok.png">
        <img class="btn-close" src="assets/images/btn-close.png">
        <img class="code" src="assets/images/code.png">
    </div>
</div>

<!--封底-->
<div class="page3 page none">
    <img class="logo" src="assets/images/logo.png">
    <img class="btn-ejiao" src="assets/images/btn-ejiao.png">
    <img class="btn-share" src="assets/images/btn-share.png">
    <div class="mask-ejiao mask none">
        <img class="code" src="assets/images/code.png">
        <img class="btn-close" src="assets/images/btn-close.png">
    </div>
    <div class="mask-share mask none">
        <img class="icon-share" src="assets/images/icon_share.png">
    </div>
</div>

<script src="assets/js/jquery.min.js"></script>
<script src="assets/js/index.js"></script>
</body>
</html>