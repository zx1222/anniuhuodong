}

.poster .poster-content {
    width: 38.8vh;
    height: 62.27vh;
    bottom: 10.38vh;
}

.poster .poster-content img {
    width: 100%;
}

/*投票生成海报--end*/
.page1 .themewords {
    width: 51.20vh;
    height: 30.84vh;
    top: 33.75vh;

