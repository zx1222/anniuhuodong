/*屏幕宽度低于450*/
@media (max-width: 413px) {
    .page2 .participant {
        padding: 10px 10PX 0;
        width: 96%;
    }

    .page2 .personage img {
        width: 80px;
        height: 80px;
    }

    .page2 hr {
        margin: 10px 0;
    }
    .page3 .personage-info {
        width: 94%;
        top: 13vh;
    }
    .page3 .personage-info-content ul {
        padding: 10px 4%;
    }
    .page3 .personage-info .dajietop {
        width: 35vh;
        height: 35vh;
    }
}


@media (max-width: 374px) {
    /*票数字号*/
    .poll {
        font-size: 15px;
    }
    .page3 .logo {
        top: 4vh;
    }

    .page3 .title {
        width: 50vh;
        height: 29vh;
        top: -1vh;
    }

    .page3 .personage-info {
        width: 94%;
        top: 13vh;
    }

    .page3 .personage-info-content ul {
        padding: 10px 5% 0;
    }

    .page3 .personage-info-content .content-title {
        font-size: 16px;
    }

    .page3 .personage-info-content .in-vote {
        margin-bottom: 5px;
    }
    .page3 .personage-info .dajietop {
        width: 30vh;
        height: 30vh;
    }

}






