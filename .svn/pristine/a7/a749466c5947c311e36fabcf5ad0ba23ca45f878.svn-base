* {
  font-family: "微软雅黑", 'Microsoft YaHei', "SimSun", Arial, Helvetica, sans-serif;
  font-size: 1em;
}

body, html {
  width: 100%;
  height: 100%;
  min-height: 100%;
  color: #333;
  position: relative;
}

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, label, form, fieldset, legend, input, button, textarea, p, section, blockquote, th, td, strong, a {
  padding: 0;
  margin: 0;
}

ul {
  list-style: none;
}

.none {
  display: none;
}

.dbw {
  display: block;
  width: 100%;
}

.page {
  width: 100%;
  min-height: 100%;
  //overflow: hidden;
}

//首页
.page1 {
  background: url("../images/p1.jpg") no-repeat;
  background-size: cover;
  .btn-start {
    position: absolute;
    width: 48%;
    left: 26%;
    bottom: 7%;
  }
  .mask-end {
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    position: absolute;
    z-index: 3;
    background: rgba(10, 10, 10, .4);
    .btn-close {
      position: absolute;
      width: 7%;
      right: 7%;
      top: 18%;
    }
    .code {
      position: absolute;
      width: 52%;
      left: 24%;
      top: 29%;
    }
    p {
      position: absolute;
      width: 100%;
      text-align: center;
      font-size: 22px;
      bottom: 27%;
      color: #fff;
    }
  }
}

.logo {
  position: absolute;
  width: 36%;
  left: 32%;
  top: 2%;
  z-index: 2;
}

//游戏
@keyframes txy1key {
  0% {
    -webkit-transform: translateY(0%);
    opacity: 1;
  }
  99% {
    -webkit-transform: translateY(-550%);
    opacity: 1;
  }
  100% {
    -webkit-transform: translateY(-550%);
    opacity: 0;
  }
}

.page2 {
  img {
    position: absolute;
  }
  .regulation {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 3;
    background: rgba(10, 10, 10, .4);
    .panel-regulation {
      width: 100%;
      top: 7%;
    }
    .btn-close {
      width: 7%;
      right: 4%;
      top: 3%;
    }
  }
  .p22 {
    width: 100%;
    height: 88%;
    top: 0;
  }
  .p23 {
    width: 100%;
    height: 12%;
    bottom: 0;
  }
  .lisCon {
    .countdown {
      position: absolute;
    }
    em {
      display: block;
      position: absolute;
      bottom: 0;
      left: 0;
      width: 18%;
      height: 4.5rem;
      -webkit-transition: opacity .2s linear;
      transition: opacity .2s linear;
      background-size: 100% 100%;
    }
    .colorAdd {
      color: #fc122e;
    }

    .colorSub {
      color: #08c46e;
    }
    .ck {
      opacity: 0;
    }
    .ck:after {
      content: attr(data-num);
      position: absolute;
      top: -.8rem;
      left: .9rem;
      width: 1.35rem;
      height: .4rem;
      line-height: .4rem;
      font-size: 1.2rem;
      text-align: center;
      font-weight: bold;
    }
    .type1 {
      background-image: url('../images/lis1.png');
    }

    .type2 {
      background-image: url('../images/lis2.png');
    }

    .type3 {
      background-image: url('../images/lis3.png');
    }

    .type4 {
      background-image: url('../images/lis4.png');
    }

    .type5 {
      background-image: url('../images/lis5.png');
    }

    .type6 {
      background-image: url('../images/lis6.png');
    }
    .type7 {
      background-image: url('../images/lis7.png');
    }
  }
  .lisCon em:nth-child(1),
  .lisCon em:nth-child(2),
  .lisCon em:nth-child(3),
  .lisCon em:nth-child(4),
  .lisCon em:nth-child(5)
   {
    left: 10%;
  }
  .lisCon em:nth-child(6),
  .lisCon em:nth-child(7),
  .lisCon em:nth-child(8),
  .lisCon em:nth-child(9),
  .lisCon em:nth-child(10)

  {
    left: 41%;
  }

  .lisCon em:nth-child(11),
  .lisCon em:nth-child(12),
  .lisCon em:nth-child(13),
  .lisCon em:nth-child(14),
  .lisCon em:nth-child(15)
  {
    left: 72%;
  }
  .lisCon .txy1 {
    -webkit-animation: txy1key 3s cubic-bezier(0.7, 0, 1, 1.01) forwards;
    -webkit-transform-origin: 50% 50%;
    -webkit-animation: txy1key 3s cubic-bezier(0.7, 0, 1, 1.01) forwards;
    animation: txy1key 7s cubic-bezier(0.7, 0, 1, 1.01) forwards;
    transform-origin: 50% 50%;
