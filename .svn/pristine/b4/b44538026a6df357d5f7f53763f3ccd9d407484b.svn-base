  .prize {
    .win {
      h3 {
        left: 37%;
        letter-spacing: 4px;
      }
    }
    .noWin {
      .code {
        bottom: 35%;
      }
      .p {
        bottom: 31%;
      }
    }
  }
}
/*x*/
