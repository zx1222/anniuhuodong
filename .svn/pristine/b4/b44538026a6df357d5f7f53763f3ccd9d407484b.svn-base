.chanceNum {
  position: absolute;
  width: 100%;
  bottom: 15%;
  text-align: center;
  font-size: 24px;
}

/*适配*/
@media screen and (min-width: 320px) and (max-width: 374px) {
  .index .swiper-slide:nth-child(1) .text {
    height: 74px;
  }

  body, html {
    font-size: 14px;
  }

  .text-l1 {
    font-size: 56px;
  }

  .text-l2 {
    font-size: 26px;
  }

  .text-l3 {
    font-size: 28px;
  }

  .text-m {
    font-size: 22px;
  }

  .prize {
    .win {
      .title {
        top: 25%;
        left: 33.5%;
        letter-spacing: 3px;
      }
    }
    .noWin {
      .sub-title {
        font-size: 14px;
      }
      .content {
        top: 35%;
        font-size: 12px;
      }
    }
  }
}

