@-webkit-keyframes twinkling{   /*透明度由0到1*/
   
    0%{
    opacity:.6; /*透明度为1*/
    }
    50%{
    opacity:.2; /*透明度为1*/
    }
     100%{
    opacity:.6; /*透明度为1*/
    }
    }
