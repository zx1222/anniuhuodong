* {
    margin: 0;
    padding: 0;
}

html,
body {
    width: 100%;
    height: 100%;
    font-family: hanyi;
}

body {
    position: fixed;
}

img {
    vertical-align: middle;
    border: 0;
    outline: 0;
    display: inline-block;
}

a {
    text-decoration: none;
    display: block;
}

@font-face
{
    font-family: hanyi;
    src: url('../fonts/hyqihei.ttf');
}
/*文字颜色*/
.text-brown{
    color: #96804b;
}
.text-darkBrown{
    color: #4c390a;
}
/*文字颜色结束*/

/*动画开始*/
@keyframes twinkle {
    0%{
        opacity: 1;
    }
    50%{
        opacity: .4;
    }
    100%{
        opacity: 1;
    }
}
.twinkle{
    animation: twinkle 1.6s linear infinite;
    -webkit-animation: twinkle 1.6s linear infinite;
}
@keyframes slideLoop {
    from{
        right: 10%;
    }
    to{
        right: 66%;
    }
}
.slideLoop{
    animation: slideLoop 1.6s linear infinite;
    -webkit-animation: slideLoop 1.6s linear infinite;
}

@keyframes bounceIn {
    from, 20%, 40%, 60%, 80%, to {
        animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
    }

    0% {
        opacity: 0;
        transform: scale3d(.3, .3, .3);
    }

    20% {
        transform: scale3d(1.1, 1.1, 1.1);
    }

    40% {
        transform: scale3d(.9, .9, .9);
    }

    60% {
        opacity: 1;
        transform: scale3d(1.03, 1.03, 1.03);
    }

    80% {
        transform: scale3d(.97, .97, .97);
    }

    to {
        opacity: 1;
        transform: scale3d(1, 1, 1);
    }
}

.bounceIn {
    animation: bounceIn .6s linear 1;
    -webkit-animation: bounceIn .6s linear 1;
    animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}

@keyframes scale {
    0%{
        transform:scale(2.2)
    }
    20%{
        transform:scale(2.8)
    }
    45%{
        transform:scale(2.4)
    }
    60%{
        transform:scale(2.7)
    }
    100%{
        transform:scale(2.55)
    }
}
.scale{
    animation: scale .6s ease-in-out 1;
    -webkit-animation: scale .6s ease-in-out 1;
    animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}
@keyframes fadeIn {
    from {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

.fadeIn {
    animation: fadeIn .4s linear 1;
    -webkit-animation: fadeIn .4s linear 1;
    animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}

/*动画结束*/
.swiper-container,.container {
    width: 100%;
    height: 100%;
    position: fixed;
    background: url("../img/bg.jpg") no-repeat;
    background-size: cover;
}

.swiper-wrapper,.container {
    width: 100%;
    height: 100%;
}

.swiper-slide {
    width: 100%;
    height: 100%;
}
/*首页开始*/
.swiper-slide:nth-of-type(1)>img{
    position: absolute;
}
.logo{
    position: absolute;
    top:1%;
    width: 62%;
    left: 19%;
    z-index: 2;
}
.swiper-slide:nth-of-type(1)>.text-them{
    width: 78%;
    left: 17%;
    top:9%;
}
.swiper-slide:nth-of-type(1)>img:nth-child(3){
    width: 100%;
    left: 0;
    top: 43%;
}
.swiper-slide:nth-of-type(1)>img:nth-child(4){
    width: 30%;
    left: 47%;
    top: 63%;
    z-index: 2;
}
.swiper-slide:nth-of-type(1)>img:nth-child(5){
    width: 64%;
    left: 18%;
    bottom:8%;
}
.swiper-slide:nth-of-type(1)>img:nth-child(6){
    width: 19%;
    right: 10%;
    bottom:0;
}

.swiper-slide:nth-of-type(1)>.regulation{
    width: 100%;
    height: 100%;
    top:0;
    z-index: 3;
}
.swiper-slide:nth-of-type(1)>.btn-know{
    width: 28%;
    left: 36%;
    bottom: 25%;
    z-index: 4;
}
/*首页结束*/
/*列表页开始*/
.swiper-slide:nth-child(2){
    background: url('../img/bg2.jpg') no-repeat;
    background-size: cover;
}
.swiper-slide:nth-child(2)>img:nth-child(2){
    position: absolute;
    bottom: 1%;
    width: 62%;
    left: 19%;
}
.swiper-slide:nth-child(2)>.content{
    position: relative;
    width: 86%;
    height:90%;
    margin-left: 7%;
    margin-top: 12%;
    overflow: scroll;
}
.swiper-slide:nth-child(2)>.content>.box{
    position: relative;
    width: 100%;
    height: auto;
    overflow: hidden;
    margin-bottom: 8px;
}
.content>.box>span{
    display: block;
}
.content>.box>a>.l{
    display: block;
    line-height: 30px;
    font-weight: 600;
    float: left;
    color: #fff;
    font-size: 18px;
    padding-left: 10px;
}
.content>.box>.r{
    width: 100%;
    height: 16px;
    text-align: right;
    font-size: 14px;
    display: flex;
    align-items: flex-end;
    justify-content: flex-end;
    color: #1b1b1b;
}
.content>.box>a{
    width: 100%;
    height: 30px;
    float: left;
    position: relative;
    background:url("../img/p2_3.png") no-repeat;
    background-size: 100% 100%;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.content>.box>a>.title{
    position: absolute;
    width: 72%;
    height: 100%;
    color: #fff;
    font-size: 16px;
    text-align: left;
    line-height: 30px;
    padding-left: 15px;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
/*列表页结束*/

/*详情页开始*/
.article{
    width: 100%;
    height: 100%;
    background: url("../img/p3_1.png");
    background-size: 100% 100%;
}
.article>.rem{
    position: absolute;
    top:0;
    width: 100%;
    height: 100%;
    z-index: 2;
}
.article>.num{
    position: absolute;
    width: 90px;
    left: calc(50% - 45px);
    top:12%;
    color: #fff;
    background: #a40c0e;
    padding: 2px;
    box-sizing: border-box;
    font-size: 22px;
    text-align: center;
}
.article>.title{
    position: absolute;
    top:19.5%;
    width: 84%;
    left: 8%;
    text-align: center;
}
.article>.author{
    position: absolute;
    top:27%;
    width: 100%;
    text-align: center;
    font-size: 16px;
}
.article>.content{
    position: absolute;
    width: 84%;
    left: 9%;
    top:33%;
    height: 46%;
    background: #fff;
    box-shadow: 0 0 8px rgba(100,59,10,.26) inset,
    0 0 16px rgba(100,59,10,.26) inset,
    0 0 8px rgba(100,59,10,.26) inset,
    0px 0px 0px rgb(255,255,255) inset;
    overflow: scroll;
}
.article>.content>img{
    width: 100%;
}
.article>.currentPoll{
    position: absolute;
    bottom: 15%;
    width: 100%;
    font-size: 18px;
    text-align: center;
    letter-spacing: 1px;
}
.article>.btn-back{
    position: absolute;
    width: 20%;
    left: 18%;
    bottom: 7%;
}
.article>.btn-vote{
    position: absolute;
    width: 22%;
    right: 18%;
    bottom: 7%;
}
.article>.btn-back>img,.article>.btn-vote>img{
    width: 100%;
}

.article>.shade,.article>.shade{
    position: absolute;
    width: 100%;
    height: 100%;
}
.article>.shade-success>img,.article>.shade-fail>img{
    width: 100%;
    height: 100%;
    position: absolute;
    top:0
}
.article>.shade>a{
    position: absolute;
    width: 30px;
    right: 11%;
    top:21%;
}
.article>.shade>a>img{
    width: 100%;
}
/*详情页结束*/

/*分享页开始*/
.share{
    width: 100%;
    height: 100%;
    background-size: 100% 100%;
}
.share img{
    position: absolute;
}
.share>img:nth-child(2){
    width: 78%;
    left: 17%;
    top:9%;
}
.share>img:nth-child(3){
     width:48%;
     left: 27%;
     top:42%;
 }
.share>img:nth-child(4){
    width:36%;
    left: 32%;
    bottom:17%;
}
.share>img:nth-child(5){
    width:100%;
    height: 100%;
    top: 0;
    z-index: 3;
}
.share>.btn-backList{
    position: absolute;
    width:36%;
    height: 40px;
    left: 32%;
    bottom:7%;
}
.share>.btn-backList>img{
    width: 100%;
}
/*分享页结束*/

/*6*/

@media only screen and (min-device-width: 375px) and (max-device-width: 413px) and (orientation : portrait) {
    .content>.box>a{
        height: 26px;
    }
    .content>.box>a>.title{
        line-height: 26px;
    }.content>.box>a>.l{
         line-height: 26px;
     }
    .content>.box>.r{
        font-size: 12px;
    }
    .swiper-slide:nth-child(2)>.content{
        margin-top: 12%;
        height: 90%;
    }
}
/*6+*/
@media only screen and (min-device-width: 414px) and (max-device-width: 736px) and (orientation : portrait) {
    .content>.box>a{
        height: 30px;
    }
    .content>.box>a>.title{
        line-height: 30px;
    }.content>.box>a>.l{
         line-height: 30px;
     }
    .content>.box>.r{
        font-size: 14px;
        height: 18px;
    }
    .swiper-slide:nth-child(2)>.content{
        margin-top: 12%;
        height: 88%;
    }
}
