body {

    font-family: 'Microsoft YaHei', Arial;
    position: fixed;

    overflow: auto;

    width: 100%;
    height: 100%;
    background-image: url("../images/bg.png");

}

@-webkit-keyframes twinkling {
    /*透明度由0到1*/

    0% {
        opacity: .6; /*透明度为1*/
    }
    50% {
        opacity: .2; /*透明度为1*/
    }
    100% {
        opacity: .6; /*透明度为1*/
    }
}

/*阿胶大姐卡通人物进入动态效果*/

.animated {
    animation-duration: 1s;
    animation-fill-mode: both;
}

@keyframes fadeInLeft {
    0% {
        opacity: .5;
        transform: translate3d(-50%, 0px, 0px);
    }
    100% {
        opacity: 1;
        transform: none;
    }
}

@keyframes fadeInLeft {
    0% {
        opacity: .5;
        transform: translate3d(-50%, 0px, 0px);
    }
    100% {
        opacity: 1;
        transform: none;
    }
}

.fadeInLeft {
    animation-name: fadeInLeft;
}

.u-pageLoading {
    width: 100%;
    height: 100%;
}

.u-pageLoading img {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;

    width: 25%;
    margin: auto;
}

.logo {
    position: fixed;
    z-index: 9;
    top: 4.46vh;
    left: 0;
    right: 0;
    margin: auto;

    width: 19.98vh;
    height: 5.05vh;

    background-image: url(../images/logo.png);
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100% 100%;
}

#arrow {
    position: fixed;
    z-index: 99;
    right: 0;
    bottom: 1.6583vh;
    left: 0;

    width: 6.8822vh;
    height: 3.7313vh;
    margin: 0 auto;

    -webkit-animation: twinkling 2s infinite ease-in-out;
    animation: twinkling 2s infinite ease-in-out;

    background-image: url(../images/arrow.png);
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100% 100%;
}

.bg-post {
    position: absolute;

    margin: 0 auto;

    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100% 100%;
}

.wrapper {
    position: absolute;
    top: 0;
    left: 0;

    width: 100%;
    height: 100%;
}

.page {
    position: relative;

    overflow: hidden;

    height: 100%;

    background-repeat: no-repeat;
    background-position: top center;
    background-size: 100% 100%;
}

.page img {
    -webkit-transition-duration: 2s; /* Safari 和 Chrome */
    -moz-transition-duration: 2s; /* Firefox 4 */
    -o-transition-duration: 2s;
    transition-duration: 2s;
}

.page-post-center {
    /*定位左右居中*/
    left: 0;
    right: 0;
    margin: auto;
}

/*logo-静态--begin*/
.logo-static {
    position: static;
}

/*遮罩层--begin*/
.mask {
    background-color: #000;
    width: 100%;
    height: 100%;
    opacity: 0.7;
    z-index: 10;
}

/*遮罩层--end*/
/*顶部花边--begin*/
.topstrap {
    width: 100%;
    height: 3.98vh;
    background-image: url("../images/topstrap.png");
    z-index: 5;
}

/*顶部花边--end*/
/*标题语--begin*/
.title {
    width: 50.89vh;
    height: 30.17vh;
    top: 1vh;

    background-image: url("../images/title.png");
    z-index: 3;
}

/*标题语--end*/
/*大姐卡通人物--begin*/
.ejiaodajie-icon {
    width: 29.91vh;
    height: 38.13vh;
    left: 0;
    bottom: 0;

    background-image: url("../images/ejiaodajie-icon.png");
    z-index: 3;
}

/*大姐卡通人物--end*/
/*右边房子--begin*/
.house-one {
    width: 53.20vh;
    height: 25.84vh;
    right: 0;
    bottom: 0;

    background-image: url("../images/house-one.png");
    z-index: 3;
}

/*右边房子--end*/
/*投票按钮--begin*/
.vote {
    width: 8.54vh;
    height: 3.98vh;
    background-image: url("../images/vote.png");
    background-size: 100% 100%;
}

.vote-link {
    width: 8.54vh;
    height: 3.98vh;
}

/*投票按钮--end*/
/*显示票数按钮--begin*/
.poll {
    padding: 0 12px;
    border-radius: 5px;
    height: 25px;
    color: #fff;
    text-align: center;
    font-size: 17px;
    margin-top: 1px;
    background-color: #fb331f;
}

/*显示票数按钮--end*/
/*投票生成海报--begin*/
.poster {
    width: 39.46vh;
    height: 85vh;
    top: 0;
    bottom: 0;
    background-image: url("../images/poster-bg.png");
