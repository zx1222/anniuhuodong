(-webkit-device-pixel-ratio: 3) {
  .index .swiper-slide:nth-child(1) .text {
    top: 19.5%;
  }
  .index .swiper-slide:nth-child(2) .btn-click {
    bottom: 38%;
  }

  .turntable .btn-draw {
    top: 29%;
  }

  .prize, .form, .submitOK {
    background-size: 100% 100%;
  }
  .backcover .s2 {
    top: 14%;
  }
  .backcover .s2 {
    top: 24%;
  }
}

//5.85英寸
@media only screen and (device-width: 360px) and (device-height: 640px) and
(-webkit-device-pixel-ratio: 3) {
  .index .swiper-slide:nth-child(1) .text {
