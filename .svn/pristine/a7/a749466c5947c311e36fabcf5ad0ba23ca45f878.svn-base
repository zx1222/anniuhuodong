  //领奖信息
  .mask-info {
    .panel-info {
      width: 80%;
      left: 10%;
      top: 20%;
    }
    .btn-submit {
      width: 29%;
      right: 14%;
      top: 72%;
    }
    form {
      position: relative;
      width: 56%;
      height: 200px;
      margin-left: 22%;
      margin-top: 65%;
      input {
        width: 100%;
        -webkit-box-sizing: border-box;
        box-sizing: border-box;
        border: 1px solid #be1b00;
        border-radius: 6px;
        margin-bottom: 17px;
        padding: 6px 10px;
      }
    }
  }

  //领取成功
  .mask-submitok {
    .panel-submitok {
      width: 88%;
      left: 6%;
      top: 25%;
    }
    .code {
      width: 30%;
      left: 35%;
      top: 45%;
    }
  }
}

