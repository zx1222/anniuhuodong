      bottom: -0.5%;
      z-index: 3;
      -webkit-animation: wave 6s 1.1s infinite linear;
      -moz-animation: wave 6s 1.1s infinite linear;
      animation: wave 6s 1.1s infinite linear;
    }
    .red-packet {
      width: 66%;
      left: 17%;
      bottom: -2%;
      z-index: 2;
    }
    .packet-header {
      position: relative;
      width: 57%;
      height: 77px;
      left: 21.5%;
      margin-top: 95%;
      z-index: 2;
      perspective: 800px;
      -webkit-perspective: 800px;
      -moz-perspective: 800px;
      .packet-header-front {
        position: absolute;
        top: 0;
        width: 96%;
        left: 2%;
      }
      .packet-header-front, .packet-header-back {
        -webkit-backface-visibility: hidden;
        -moz-backface-visibility: hidden;
        backface-visibility: hidden;
        -webkit-transition: all 1.2s;
        -moz-transition: all 1.2s;
        transition: all 1.2s;
        backface-visibility: hidden;
        z-index: 2;
      }
      .packet-header-back {
        width: 90%;
        left: 5%;
        transform: translate3d(0, 20%, 0);
        -webkit-transform: rotateX(180deg);
        -moz-transform: rotateX(180deg);
        transform: rotateY(180deg);
      }
    }
    .packet-header.flipped .packet-header-back {
      -webkit-transform: rotateZ(-180deg);
      -moz-transform: rotateZ(-180deg);
      transform: rotateZ(-180deg);
      transform-origin: 50% 0%;
      top: 32%;
    }
    .packet-header.flipped .packet-header-front {
      -webkit-transform: rotateX(-180deg);
      -moz-transform: rotateX(-180deg);
      transform: rotateX(-180deg);
      transform-origin: 50% 0%;
      z-index: -1;
    }
