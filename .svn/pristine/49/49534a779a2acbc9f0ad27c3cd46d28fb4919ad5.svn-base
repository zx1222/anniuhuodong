    text-align: center;
    position: absolute;
    margin-top: 17%;
    .leaveGroup-num {
      font-size: 28px;
      color: #dd3929;
      font-weight: 600;
    }
  }
  .card-container {
    overflow: hidden;
    margin-top: 49%;
    width: 96%;
    margin-left: 2%;
  }
  .card-wrp {
    width: 25%;
    float: left;
    height: auto;
    border: 5px solid transparent;
    border-style: inset;
    -webkit-transition: all 0.4s;
    -moz-transition: all 0.4s;
    transition: all 0.4s;
    position: relative;
    box-sizing: border-box;

    .card-front, .card-back {
      position: absolute;
      width: 100%;
      height: 100%;
      -webkit-backface-visibility: hidden;
      -moz-backface-visibility: hidden;
      backface-visibility: hidden;
      -webkit-transition: all 0.4s;
      -moz-transition: all 0.4s;
      transition: all 0.4s;
      img {
        width: 100%;
      }
    }

    .card-back {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      -webkit-transform: rotateY(-180deg);
      -moz-transform: rotateY(-180deg);
      transform: rotateY(-180deg);
    }

  }

  .flipped .card-back {
    -webkit-transform: rotateY(0deg);
    -moz-transform: rotateY(0deg);
    transform: rotateY(0deg);
  }
  .success .card-back {
    -webkit-transform: rotateY(0deg);
    -moz-transform: rotateY(0deg);
    transform: rotateY(0deg);
  }

  .flipped .card-front {
    -webkit-transform: rotateY(180deg);
    -moz-transform: rotateY(180deg);
    transform: rotateY(180deg);
  }
  .success .card-front {
    -webkit-transform: rotateY(180deg);
    -moz-transform: rotateY(180deg);
    transform: rotateY(180deg);
  }
