//转盘
.turntable {
  width: 100%;
  height: 100%;
  background: url("../images/bg03.jpg") no-repeat;
  background-size: cover;
  z-index: 3;
  img {
    position: absolute;
  }
  .turntable-panel {
    width: 92%;
    left: 5%;
    top: 14%;
  }
  .btn-draw {
    position: absolute;
    width: 24%;
    left: 39%;
    top: 32.3%;
  }
}

//奖品
.prize {
  background: url("../images/bg04.jpg") no-repeat;
  background-size: cover;
  img {
    position: absolute;
  }
  .win {
    display: none;
    .title {
      border-bottom: 1px solid #c52d44;
      position: absolute;
      top: 26%;
      left: 36%;
      letter-spacing: 3px;
    }
    .prize-product {
      width: 40%;
      left: 30%;
      top: 35%
    }
    .prize-name {
      position: absolute;
      width: 100%;
      text-align: center;
      top: 53%;
      left: 0;
      letter-spacing: 2px;
    }
    .btn-get {
      width: 28%;
      left: 36%;
      bottom: 33%;
    }
  }
  .noWin {
    .title {
      position: absolute;
      width: 34%;
      left: 33%;
      border-bottom: 1px solid #c52d44;
      top: 24%;
      letter-spacing: 4px;
    }
    .sub-title {
      position: absolute;
      width: 100%;
      left: 0;
      top: 31%;
    }
    .content {
      position: absolute;
      width: 42%;
      left: 29%;
      top: 36%;
      color: #212121;
      letter-spacing: 1px;
      opacity: 0;
    }
    .code {
      position: absolute;
      width: 26%;
      left: 37%;
      bottom: 33%;
    }
    p {
      position: absolute;
      width: 100%;
      left: 0;
      font-size: 14px;
      bottom: 29%;
    }
    .btn-close {
      position: absolute;
      width: 7%;
      right: 8%;
      top: 8%;
    }

  }
}

.form {
  background: url("../images/bg04.jpg") no-repeat;
  background-size: cover;
  width: 100%;
  height: 100%;
  .logo {
    position: relative;
    width: 34%;
    margin-left: 32%;
    margin-top: 3%;
    z-index: 2;
    top: 0;
    left: 0;
  }
  .title {
    width: 44%;
    margin-left: 28.5%;
    margin-top: 28%;
    border-bottom: 1px solid #c52d44;
    letter-spacing: -.5px;
  }
  form {
    width: 36%;
    margin-left: 32%;
    padding-top: 8%;
  }
  input {
    background: #fff;
    border: 1px solid #c52d44;
    border-radius: 6px;
    padding: 6px;
    width: 100%;
    box-sizing: border-box;
    margin-bottom: 20px;
    flex: 1;
  }
  .btn-submit {
    width: 26%;
    margin-left: 7%;
  }
}

.submitOK {
  background: url("../images/bg04.jpg") no-repeat;
  background-size: cover;
  z-index: 3;
  .title {
    position: absolute;
    width: 32%;
    left: 33%;
    top: 24%;
    border-bottom: 1px solid #c52d44;
    letter-spacing: 3px;
  }
  .code {
    position: absolute;
    width: 28%;
    left: 35%;
